{
	"manifest_version": 2,
	"name": "cast",
	"version": "0.1",

	"description": "Allows casting of videos to computers running the cast software.",

	"browser_action": {
		"default_icon": "icons/ic_cast_3x.png",
		"default_title": "cast",
		"default_popup": "popup/cast.html"
	},

	"permissions": [
		"storage",
		"tabs",
		"activeTab"
	],


	"icons": {
		"48": "icons/cast_icon.png"
	},

	"web_accessible_resources": [
		"icons/forward_icon.png",
		"icons/replay_icon.png",
		"icons/play_icon.png",
		"icons/pause_icon.png",
		"icons/cast_icon.png"
	]

}

