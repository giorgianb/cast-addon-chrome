{
	"manifest_version": 2,
	"name": "cast",
	"version": "0.1",

	"description": "Allows casting of videos to computers running the cast software.",

	"browser_action": {
		"default_icon": "icons/ic_cast_3x.png",
		"default_title": "cast",
		"default_popup": "popup/cast.html"
	},

	"permissions": [
		"storage",
		"tabs",
		"activeTab"
	],


	"icons": {
		"48": "icons/ic_cast_3x.png"
	},

	"web_accessible_resources": [
    "icons/ic_cast_3x.png",
    "icons/ic_fast_forward_3x.png",
    "icons/ic_fast_rewind_3x.png",
    "icons/ic_pause_3x.png",
    "icons/ic_play_arrow_3x.png",
    "icons/ic_subtitles_3x.png",
    "icons/ic_volume_down_3x.png",
    "icons/ic_volume_up_3x.png"
	]
}

